CONFIG_LOW_LEVEL_OPTIONS=y
CONFIG_MACH_STM32=y
CONFIG_BOARD_DIRECTORY="stm32"
CONFIG_MCU="stm32f446xx"
CONFIG_CLOCK_FREQ=180000000
CONFIG_FLASH_SIZE=0x80000
CONFIG_FLASH_BOOT_ADDRESS=0x8000000
CONFIG_RAM_START=0x20000000
CONFIG_RAM_SIZE=0x20000
CONFIG_STACK_SIZE=512
CONFIG_FLASH_APPLICATION_ADDRESS={{ '0x8008000' if deployer else '0x8000000' }}
CONFIG_FLASH_START=0x8000000
CONFIG_LAUNCH_APP_ADDRESS=0x8008000
CONFIG_BLOCK_SIZE=64
CONFIG_STM32_SELECT=y
CONFIG_MACH_STM32F446=y
CONFIG_MACH_STM32F4=y
CONFIG_HAVE_STM32_USBOTG=y
CONFIG_HAVE_STM32_CANBUS=y
CONFIG_STM32_DFU_ROM_ADDRESS=0
CONFIG_STM32_FLASH_START_{{ '8000' if deployer else '0000' }}=y
CONFIG_STM32_CLOCK_REF_12M=y
CONFIG_CLOCK_REF_FREQ=12000000
CONFIG_STM32F0_TRIM=16
CONFIG_STM32_MMENU_CANBUS_PD0_PD1=y
CONFIG_STM32_CANBUS_PD0_PD1=y
CONFIG_STM32_APP_START_8000=y
CONFIG_USB_VENDOR_ID=0x1d50
CONFIG_USB_DEVICE_ID=0x6177
CONFIG_USB_SERIAL_NUMBER="12345"
CONFIG_CANSERIAL=y
CONFIG_CANBUS=y
CONFIG_CANBUS_FREQUENCY={{bitrate|default(1000000)|int}}
CONFIG_CANBUS_FILTER=y
CONFIG_INITIAL_PINS=""
CONFIG_ENABLE_DOUBLE_RESET=y
# CONFIG_ENABLE_BUTTON is not set
{% if deployer %}
CONFIG_BUILD_DEPLOYER=y
{% else %}
CONFIG_HAVE_CHIPID=y